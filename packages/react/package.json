{
  "name": "@josui/react",
  "version": "0.3.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src": "./src/index.ts"
  },
  "files": [
    "dist",
    "skills",
    "src",
    "tailwind.css"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist storybook-static",
    "lint": "eslint src",
    "test": "vitest run",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@josui/core-web": "workspace:*"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@josui/tailwind": "workspace:*",
    "@josui/typescript-config": "workspace:*",
    "@storybook/addon-a11y": "^10.2.0",
    "@storybook/addon-docs": "^10.2.0",
    "@storybook/addon-links": "^10.2.0",
    "@storybook/react-vite": "^10.2.0",
    "@tailwindcss/vite": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.6.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@josui/prettier-config": "workspace:*",
    "jsdom": "^26.0.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "storybook": "^10.2.0",
    "tailwindcss": "^4.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
