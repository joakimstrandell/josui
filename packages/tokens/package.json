{
  "name": "@josui/tokens",
  "version": "0.3.1",
  "type": "module",
  "main": "./dist/js/index.js",
  "module": "./dist/js/index.js",
  "types": "./dist/js/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/js/index.d.ts",
      "import": "./dist/js/index.js"
    },
    "./css": {
      "style": "./dist/css/variables.css",
      "default": "./dist/css/variables.css"
    },
    "./tailwind": {
      "style": "./dist/tailwind/theme.css",
      "default": "./dist/tailwind/theme.css"
    },
    "./scss": {
      "style": "./dist/scss/index.scss",
      "default": "./dist/scss/index.scss"
    }
  },
  "files": [
    "dist",
    "skills"
  ],
  "scripts": {
    "build": "tz build && node -e \"const fs=require('fs');const f='./dist/tailwind/theme.css';fs.writeFileSync(f,fs.readFileSync(f,'utf8').replace('@import \\\"tailwindcss\\\";\\n\\n',''))\"",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@terrazzo/cli": "^0.10.5",
    "@terrazzo/plugin-css": "^0.10.4",
    "@terrazzo/plugin-js": "^0.10.3",
    "@terrazzo/plugin-sass": "^0.10.4",
    "@terrazzo/plugin-tailwind": "^0.3.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
