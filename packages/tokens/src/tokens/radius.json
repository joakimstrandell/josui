{
  "radius": {
    "$type": "dimension",
    "none": {
      "$value": { "value": 0, "unit": "px" },
      "$description": "No border radius"
    },
    "sm": {
      "$value": { "value": 0.125, "unit": "rem" },
      "$description": "Small radius (2px)"
    },
    "default": {
      "$value": { "value": 0.375, "unit": "rem" },
      "$description": "Default radius (6px)"
    },
    "md": {
      "$value": { "value": 0.5, "unit": "rem" },
      "$description": "Medium radius (8px)"
    },
    "lg": {
      "$value": { "value": 0.75, "unit": "rem" },
      "$description": "Large radius (12px)"
    },
    "xl": {
      "$value": { "value": 1, "unit": "rem" },
      "$description": "Extra large radius (16px)"
    },
    "2xl": {
      "$value": { "value": 1.5, "unit": "rem" },
      "$description": "2XL radius (24px)"
    },
    "3xl": {
      "$value": { "value": 1.875, "unit": "rem" },
      "$description": "3XL radius (30px)"
    },
    "full": {
      "$value": { "value": 9999, "unit": "px" },
      "$description": "Full/pill radius"
    }
  }
}
